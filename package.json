{
  "name": "ngx-api-utils-source",
  "version": "0.0.0",
  "private": true,
  "author": "Radoslav Kirilov <rkirilow@gmail.com> (https://github.com/smoke)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ngx-api-utils/ngx-api-utils.git"
  },
  "engines": {
    "node": ">=8.11.0",
    "npm": ">=6.2.0"
  },
  "scripts": {
    "ng": "ng",
    "start": "npm-run-all -p -l fake-api:start ngx-api-utils:start demo:start",
    "build": "npm-run-all -l \"ngx-api-utils:build -- {@}\" \"demo:build -- {@}\" --",
    "test": "npm-run-all \"ngx-api-utils:test -- {@}\" --",
    "lint": "npm-run-all -p -l ngx-api-utils:lint demo:lint",
    "e2e": "npm-run-all -l \"ngx-api-utils:build\" -p -r fake-api:start \"demo:e2e -- {@}\" --",
    "fake-api:start": "nodemon --watch fake-api-jwt-json-server -e '*' -x npm-run-all -- fake-api:start:server",
    "fake-api:start:server": "node fake-api-jwt-json-server/server.js",
    "demo:start": "ng serve ngx-api-utils-demo",
    "demo:build": "ng build ngx-api-utils-demo",
    "demo:lint": "ng lint ngx-api-utils-demo",
    "demo:e2e": "ng e2e ngx-api-utils-demo",
    "ngx-api-utils:start": "nodemon --watch projects/ngx-api-utils -e '*' -i '*.metadata.json' -x npm-run-all -- ngx-api-utils:build",
    "ngx-api-utils:start-incremental-build": "echo \"Until https://github.com/angular/angular-cli/pull/11358 is merged this won't work so by then use 'ngx-api-utils:start'\n\"; exit; npm-run-all ngx-api-utils:build -- --watch {@} --",
    "ngx-api-utils:build": "ng build ngx-api-utils",
    "ngx-api-utils:test": "ng test ngx-api-utils",
    "ngx-api-utils:lint": "ng lint ngx-api-utils",
    "ngx-api-utils:docs": "cp README.md dist/ngx-api-utils",
    "ngx-api-utils:version": "cd projects/ngx-api-utils && npm version",
    "ngx-api-utils:publish": "cd dist/ngx-api-utils && npm publish",
    "ngx-api-utils:git-tag": "npm-run-all \"git -- commit -a -m 'release(ngx-api-utils): {1}'\" \"git -- tag {1}\" \"git -- push --no-verify origin {1}\" --",
    "ngx-api-utils:release": "npm-run-all ngx-api-utils:lint \"ngx-api-utils:test -- --watch false\" \"ngx-api-utils:version -- {1}\" \"ngx-api-utils:build -- --prod\" ngx-api-utils:docs \"ngx-api-utils:publish -- {2} {3}\" \"ngx-api-utils:git-tag -- {1}\" --",
    "prettier": "prettier",
    "format:prettier:lint-all": "prettier -l '**/*.{ts*,js*}'",
    "format:prettier:fix-all-DANGEROUSLY": "npm-run-all \"format:prettier:lint-all -- --write\"",
    "format:prettier:fix-staged": "pretty-quick --staged",
    "ci:hooks:pre-commit": "npm-run-all format:prettier:fix-staged",
    "ci:hooks:pre-push": "npm-run-all lint",
    "git": "git"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm-run-all ci:hooks:pre-commit",
      "pre-push": "npm-run-all ci:hooks:pre-push"
    }
  },
  "dependencies": {
    "@angular/animations": "~11.2.14",
    "@angular/common": "~11.2.14",
    "@angular/compiler": "~11.2.14",
    "@angular/core": "~11.2.14",
    "@angular/forms": "~11.2.14",
    "@angular/platform-browser": "~11.2.14",
    "@angular/platform-browser-dynamic": "~11.2.14",
    "@angular/router": "~11.2.14",
    "bootstrap": "^4.5.3",
    "core-js": "^3.6.5",
    "jwt-decode": "^3.0.0",
    "rxjs": "~6.6.3",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1102.18",
    "@angular/cli": "~11.2.18",
    "@angular/compiler-cli": "~11.2.14",
    "@angular/language-service": "~11.2.14",
    "@pollyjs/adapter-fetch": "^5.0.0",
    "@pollyjs/adapter-xhr": "^5.0.1",
    "@pollyjs/core": "^5.0.0",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/jwt-decode": "^2.2.1",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "husky": "^4.3.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "json-server": "^0.16.2",
    "jsonwebtoken": "^8.5.1",
    "karma": "~6.3.12",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^11.2.4",
    "nodemon": "^2.0.6",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.2",
    "pretty-quick": "^3.1.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tsickle": "^0.36.0",
    "tslib": "^1.10.0",
    "tslint": "~6.1.0",
    "tslint-plugin-prettier": "^2.3.0",
    "typescript": "~4.0.2"
  }
}
